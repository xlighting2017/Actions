CONFIG_PREINITOPT=y
CONFIG_TARGET_PREINIT_IP="192.168.199.253"
CONFIG_TARGET_PREINIT_NETMASK="255.255.255.0"
CONFIG_TARGET_PREINIT_BROADCAST="192.168.199.255"
